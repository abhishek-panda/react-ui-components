{
  "name": "react-ui-components",
  "version": "0.0.0-development",
  "description": "reac",
  "main": "index.js",
  "scripts": {
    "storybook": "start-storybook",
    "lint": "eslint --fix components/*",
    "test": "jest",
    "build": "build-storybook -c .storybook -o public",
    "start": "node server/index.js",
    "release": "semantic-release"
  },
  "repository": {
